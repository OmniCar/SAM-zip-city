{
  "name": "@omnicar/sam-zip-city",
  "version": "0.0.1",
  "description": "A simple tool for looking up city names based on zipcode inputs.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "keywords": [
    "zipcode",
    "typescript"
  ],
  "author": "Erik Beuschau",
  "license": "MIT",
  "dependencies": {
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/node": "^10.7.1",
    "jest": "^23.5.0",
    "ts-jest": "^23.1.3",
    "typescript": "^3.0.1"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(ts)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "automock": false,
    "testPathIgnorePatterns": [
      "./dist"
    ]
  },
  "scripts": {
    "tsc": "rm -rf dist/ && node node_modules/typescript/bin/tsc -p ./tsconfig.json -w",
    "tsc-once": "rm -rf dist/ && node node_modules/typescript/bin/tsc -p ./tsconfig.json",
    "test": "jest",
    "test-watch": "jest --watchAll",
    "test-debug": "node --inspect node_modules/.bin/jest --watchAll"
  }
}
