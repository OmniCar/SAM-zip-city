{
  "name": "@omnicar/sam-zip-city",
  "description": "A simple tool for looking up city names based on zipcode inputs.",
  "version": "1.0.0",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "keywords": [
    "zipcode",
    "typescript"
  ],
  "author": "Erik Beuschau, Marko K. Sepp√§nen",
  "license": "MIT",
  "dependencies": {
    "node-fetch": "^2.6.11",
    "tslib": "^1.9.3",
    "@types/node-fetch": "^2.6.4"
  },
  "devDependencies": {
    "@omnicar/sam-types": "0.0.641",
    "@types/jest": "24.0.11",
    "@types/node": "^20.0.0",
    "jest": "^24.7.0",
    "ts-jest": "^24.0.1",
    "typescript": "^4.1.6"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(ts)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "automock": false,
    "testPathIgnorePatterns": [
      "./dist"
    ]
  },
  "scripts": {
    "cleanup": "rm -rf dist/",
    "tsc": "node node_modules/typescript/bin/tsc -p ./tsconfig.json",
    "tscw": "node node_modules/typescript/bin/tsc -p ./tsconfig.json -w",
    "tsc-once": "yarn cleanup && yarn tsc",
    "test": "jest",
    "test-watch": "jest --watchAll",
    "test-debug": "node --inspect node_modules/.bin/jest --watchAll"
  }
}
